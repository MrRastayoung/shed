########################################################################
# Sample project Shedfile
########################################################################

# images to pull
IMAGES=(
	'busybox:latest'
)

# directory names for images to build
BUILDS=(
	'.'
)

# containers to create (order is significant)
CONTAINERS=(
	'echo'
	'receiver'
)

# named groups of containers
GROUPS=( 
	['echo']="${CONTAINERS[@]}"
)


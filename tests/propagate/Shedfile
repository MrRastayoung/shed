# prefixed to container and image names
PROJECT='p'

# images to pull
IMAGES=( 'busybox:latest' )

# containers to create
CONTAINERS=( a c e d b h i g f )	# post-order

# named groups of containers
GROUPS=(
	[$PROJECT]="${CONTAINERS[@]}"
)

